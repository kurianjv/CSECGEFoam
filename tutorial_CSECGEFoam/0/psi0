FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      psi0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top_wall
    {
        type            zeroGradient;
    }
    bottom_wall
    {
        type            zeroGradient;
    }
    left_wall
    {
        type            zeroGradient;
    }
    right_wall
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


